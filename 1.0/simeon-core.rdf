    <owl:Class rdf:about="http://purl.org/ceu/simeon#NormalizedComparisonMethod">
        <rdfs:label xml:lang="en">Normalized comparison method</rdfs:label>
        <rdfs:label xml:lang="es">Método de comparación normalizado</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/ceu/simeon#ComparisonMethod"/>
        <rdfs:comment xml:lang="en">A normalized comparison method is a comparison method that returns a number in the real interval [0,1].</rdfs:comment>
        <rdfs:comment xml:lang="es">Un método de comparación normalizado es un método de comparación que devuelve un número real en el intervalo [0,1].</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ceu/simeon#NormalizedDissimilarityFunction -->

    <owl:Class rdf:about="http://purl.org/ceu/simeon#NormalizedDissimilarityFunction">
        <rdfs:label xml:lang="en">Normalized dissimilarity function</rdfs:label>
