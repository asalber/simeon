<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://purl.org/ceu/lco#"
     xml:base="http://purl.org/ceu/lco"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://purl.org/ceu/simeon">
        <rdfs:label xml:lang="en">SIMEON Ontology</rdfs:label>
        <rdfs:label xml:lang="es">Ontología SIMEON</rdfs:label>
        <dc:license xml:lang="en">Attribution-ShareAlike 4.0 International Creative Commons license (http://creativecommons.org/licenses/by-sa/4.0/)</dc:license>
        <rdfs:comment xml:lang="en">SIMEON ontology allow to establish similarity measures between ontology objets and collections.</rdfs:comment>
        <dc:abstract xml:lang="en">SIMEON ontology allow to establish similarity measures between ontology objets and collections.</dc:abstract>
        <owl:versionInfo xml:lang="en">Version 1.0 2015/07/01</owl:versionInfo>
        <dc:creator xml:lang="es">Alfredo Sánchez Alberca (asalber@ceu.es) &lt;http://aprendeconalf.es/foaf.rdf&gt;</dc:creator>
        <rdfs:comment xml:lang="es">La ontología SIMEON permite establecer medidas de similitud entre los objetos y colecciones de objetos de las ontologías.</rdfs:comment>
        <dc:abstract xml:lang="es">La ontología SIMEON permite establecer medidas de similitud entre los objetos y colecciones de objetos de las ontologías.</dc:abstract>
        <dc:license xml:lang="es">Licencia de Creative Commons Reconocimiento-CompartirIgual 4.0 Internacional (http://creativecommons.org/licenses/by-sa/4.0/).</dc:license>
        <owl:versionInfo xml:lang="es">Versión 1.0 01/07/2015</owl:versionInfo>
        <owl:versionIRI rdf:resource="http://purl.org/ceu/simeon/1.0"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ceu/simeon#comparesObject -->

    <owl:ObjectProperty rdf:about="http://purl.org/ceu/simeon#comparesObject">
        <rdfs:label xml:lang="en">compares object</rdfs:label>
        <rdfs:label xml:lang="es">compara objeto</rdfs:label>
        <rdfs:comment xml:lang="en">Property that relates a comparison method with an individual that is compared with that method.</rdfs:comment>
        <rdfs:comment xml:lang="es">Propiedad que relaciona un método de comparación con un individuo que es comparado mediante ese método.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ComparisonMethod"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/ceu/simeon#usedByComparisonMeasure"/>
            <rdf:Description rdf:about="http://purl.org/ceu/simeon#hasTargetObject"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/ceu/simeon#usedByComparisonMeasure"/>
            <rdf:Description rdf:about="http://purl.org/ceu/simeon#hasSourceObject"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ceu/simeon#hasAlgorithm -->

    <owl:ObjectProperty rdf:about="http://purl.org/ceu/simeon#hasAlgorithm">
        <rdfs:label xml:lang="en">has algorithm</rdfs:label>
        <rdfs:label xml:lang="es">tiene algoritmo</rdfs:label>
        <rdfs:comment xml:lang="en">Property that relates a comparison method with the algorithm or calculation procedure that uses.</rdfs:comment>
        <rdfs:comment xml:lang="es">Propiedad que relaciona un método de comparación con el algoritmo o procedimiento de cálculo que utiliza.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/ceu/simeon#Algorithm"/>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ComparisonMethod"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ceu/simeon#hasDomain -->

    <owl:ObjectProperty rdf:about="http://purl.org/ceu/simeon#hasDomain">
        <rdfs:label xml:lang="en">has domain</rdfs:label>
        <rdfs:label xml:lang="es">tiene dominio</rdfs:label>
        <rdfs:comment xml:lang="en">Property that relates a comparison method with the types of objects or entities that compares.</rdfs:comment>
        <rdfs:comment xml:lang="es">Propiedad que relaciona un método de comparación con el tipo de objetos o entidades que compara.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ComparisonMethod"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ceu/simeon#hasReference -->

    <owl:ObjectProperty rdf:about="http://purl.org/ceu/simeon#hasReference">
        <rdfs:label xml:lang="en">has reference</rdfs:label>
        <rdfs:label xml:lang="es">tiene referencia</rdfs:label>
        <rdfs:comment xml:lang="en">Property that relates a comparison method with a bibliographic reference where the method is explained.</rdfs:comment>
        <rdfs:comment xml:lang="es">Propiedad que relaciona un método de comparación con una referencia bibliográfica donde se describe el método.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ComparisonMethod"/>
        <rdfs:range rdf:resource="http://purl.org/dc/terms/BibliographicResource"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ceu/simeon#hasSourceObject -->

    <owl:ObjectProperty rdf:about="http://purl.org/ceu/simeon#hasSourceObject">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has source object</rdfs:label>
        <rdfs:label xml:lang="es">tiene objeto origen</rdfs:label>
        <rdfs:comment xml:lang="en">A similarity measurement expresses the similarity degree of a source object with a target object. This property that relates a similarity measure with the source object.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una medida de similitud establece el grado de parecido de un objeto origen con otro destino. Esta propiedad relaciona la medida de similitud con el objeto origen.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ObjectComparisonMeasure"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ceu/simeon#hasTargetObject -->

    <owl:ObjectProperty rdf:about="http://purl.org/ceu/simeon#hasTargetObject">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has target object</rdfs:label>
        <rdfs:label xml:lang="es">tiene objeto destino</rdfs:label>
        <rdfs:comment xml:lang="en">A similarity measurement expresses the similarity degree of a source object with a target object. This property that relates a similarity measure with the target object.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una medida de similitud establece el grado de parecido de un objeto origen con otro destino. Esta propiedad relaciona la medida de similitud con el objeto destino cuando se trata de un literal.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una medida de similitud establece el grado de parecido de un objeto origen con otro destino. Esta propiedad relaciona la medida de similitud con el objeto destino.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ObjectComparisonMeasure"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ceu/simeon#isPerformedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/ceu/simeon#isPerformedBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">is performed by</rdfs:label>
        <rdfs:label xml:lang="es">es realizada por</rdfs:label>
        <rdfs:comment xml:lang="en">Property that relates a subjective comparison measure with the person that performs that measure.</rdfs:comment>
        <rdfs:comment xml:lang="es">Propiedad que relaciona una medida de comparación subjetiva con la persona que realiza esa medida.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ComparisonMeasure"/>
        <rdfs:range rdf:resource="http://purl.org/dc/terms/Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ceu/simeon#usedByComparisonMeasure -->

    <owl:ObjectProperty rdf:about="http://purl.org/ceu/simeon#usedByComparisonMeasure">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">used by comparsion measure</rdfs:label>
        <rdfs:label xml:lang="es">usada por medida de comapración</rdfs:label>
        <rdfs:comment xml:lang="en">Property that relates a comparison method with a comparison measure calculated with that method.</rdfs:comment>
        <rdfs:comment xml:lang="es">Propieda que relaciona un método de comparación con una medida de comparación calculada con ese método.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/ceu/simeon#ComparisonMeasure"/>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ComparisonMethod"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ceu/simeon#usesComparisonMethod -->

    <owl:ObjectProperty rdf:about="http://purl.org/ceu/simeon#usesComparisonMethod">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">uses comparison method</rdfs:label>
        <rdfs:label xml:lang="es">usa método de comparación</rdfs:label>
        <rdfs:comment xml:lang="en">Property that relates a comparison measure with the comparison method used to get it.</rdfs:comment>
        <rdfs:comment xml:lang="es">Propiedad que relaciona una medida de comparación con el método de comparación utilizado para obtenerla.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ComparisonMeasure"/>
        <rdfs:range rdf:resource="http://purl.org/ceu/simeon#ComparisonMethod"/>
        <owl:inverseOf rdf:resource="http://purl.org/ceu/simeon#usedByComparisonMeasure"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ceu/simeon#hasSourceData -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ceu/simeon#hasSourceData">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has source data</rdfs:label>
        <rdfs:label xml:lang="es">tiene dato origen</rdfs:label>
        <rdfs:comment xml:lang="en">A similarity measurement expresses the similarity degree of a source object with a target object. This property that relates a similarity measure with the source object when that object is a literal.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una medida de similitud establece el grado de parecido de un objeto origen con otro destino. Esta propiedad relaciona la medida de similitud con el objeto origen cuando se trata de un literal.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#DataComparisonMeasure"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ceu/simeon#hasTargetData -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ceu/simeon#hasTargetData">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has target data</rdfs:label>
        <rdfs:label xml:lang="es">tiene dato destino</rdfs:label>
        <rdfs:comment xml:lang="en">A similarity measurement expresses the similarity degree of a source object with a target object. This property that relates a similarity measure with the target object when that object is a literal.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una medida de similitud establece el grado de parecido de un objeto origen con otro destino. Esta propiedad relaciona la medida de similitud con el objeto destino cuando se trata de un literal.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#DataComparisonMeasure"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ceu/simeon#hasValue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ceu/simeon#hasValue">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has value</rdfs:label>
        <rdfs:label xml:lang="es">tiene valor</rdfs:label>
        <rdfs:comment xml:lang="en">Property that relates a similiarity measure with the value of the measure.</rdfs:comment>
        <rdfs:comment xml:lang="es">Propiedad que relaciona una medida de similitud con el valor de la medida.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ceu/simeon#ComparisonMeasure"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;decimal">0.0</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


