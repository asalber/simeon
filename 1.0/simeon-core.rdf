        <rdfs:label xml:lang="en">Subjective comparison measure</rdfs:label>
        <rdfs:label xml:lang="es">Medida de comparación subjetiva</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/ceu/simeon#ComparisonMeasure"/>
        <rdfs:comment xml:lang="en">A subjective comparison measure is a measure obtained with a subjective comparison method, that is, based on the perception, feelings or opinion of a person.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una medida de comparación subjetiva es una medida realizada con un método de comparación subjetivo, es decir, basado en la percepción, sentimientos u opinión de una persona.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ceu/simeon#SymmetricComparisonMeasure -->

    <owl:Class rdf:about="http://purl.org/ceu/simeon#SymmetricComparisonMeasure">
        <rdfs:label xml:lang="en">Symmetric comparison measure</rdfs:label>
        <rdfs:label xml:lang="es">Medida de comaración simétrica</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/ceu/simeon#ComparisonMeasure"/>
