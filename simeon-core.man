Rule: 
    hasTargetEntity(?measure1, ?entity2), SymmetricComparisonMeasure(?measure1), hasSourceEntity(?measure1, ?entity1), usesComparisonMethod(?measure1, ?method), hasValue(?measure1, ?value) -> SymmetricComparisonMeasure(?measure2), usesComparisonMethod(?measure2, ?method), hasTargetEntity(?measure2, ?entity1), hasSourceEntity(?measure2, ?entity2), hasValue(?measure2, ?value)

