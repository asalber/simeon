Class: SymmetricComparisonMethod

    Annotations: 
        rdfs:label "Método de comparación simétrico"@es,
        rdfs:comment "A symmetric comparison method is a comparison method or function c that satisfies the symmetry axiom:
c(x,y) = c(y,x) for all x,y."@en,
        rdfs:comment "Un método de comparación simétrico es un método de comparación o función c que satisface el axioma de simetría:
c(x,y) = c(y,x) para todo x,y."@es,
        rdfs:label "Symmetric comparison method"@en
    
    SubClassOf: 
        ComparisonMethod
    
    
Class: NormalizedComparisonMeasure

    Annotations: 
