#################################################################


<urn:swrl#measure> rdf:type swrl:Variable .
<urn:swrl#method> rdf:type swrl:Variable .
<urn:swrl#collection> rdf:type swrl:Variable .
[ rdf:type swrl:Imp ;
  swrl:body [ rdf:type swrl:AtomList ;
              rdf:first [ rdf:type swrl:ClassAtom ;
                          swrl:classPredicate <http://purl.org/ceu/simeon#ComparisonMeasure> ;
                          swrl:argument1 <urn:swrl#measure>
                        ] ;
              rdf:rest [ rdf:type swrl:AtomList ;
                         rdf:rest [ rdf:type swrl:AtomList ;
                                    rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                swrl:argument2 <http://purl.org/ceu/eco/1.0#Multiheteroset> ;
                                                swrl:propertyPredicate <http://purl.org/ceu/simeon#hasDomain> ;
                                                swrl:argument1 <urn:swrl#method>
                                              ] ;
                                    rdf:rest [ rdf:type swrl:AtomList ;
                                               rdf:rest rdf:nil ;
                                               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                           swrl:propertyPredicate <http://purl.org/ceu/simeon#hasSourceEntity> ;
                                                           swrl:argument2 <urn:swrl#collection> ;
                                                           swrl:argument1 <urn:swrl#measure>
                                                         ]
                                             ]
                                  ] ;
                         rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                     swrl:propertyPredicate <http://purl.org/ceu/simeon#usesComparisonMethod> ;
                                     swrl:argument1 <urn:swrl#measure> ;
                                     swrl:argument2 <urn:swrl#method>
                                   ]
                       ]
            ] ;
  swrl:head [ rdf:type swrl:AtomList ;
              rdf:rest rdf:nil ;
              rdf:first [ rdf:type swrl:ClassAtom ;
                          swrl:classPredicate <http://purl.org/ceu/eco/1.0#Multiheteroset> ;
                          swrl:argument1 <urn:swrl#collection>
                        ]
            ]
] .
[ rdf:type swrl:Imp ;
  swrl:head [ rdf:type swrl:AtomList ;
              rdf:rest rdf:nil ;
              rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                          swrl:argument2 <http://purl.org/ceu/eco/1.0#Multiheteroset> ;
                          swrl:propertyPredicate <http://purl.org/ceu/simeon#hasDomain> ;
                          swrl:argument1 <urn:swrl#method>
                        ]
            ] ;
  swrl:body [ rdf:type swrl:AtomList ;
              rdf:first [ rdf:type swrl:ClassAtom ;
                          swrl:classPredicate <http://purl.org/ceu/simeon#ComparisonMeasure> ;
                          swrl:argument1 <urn:swrl#measure>
                        ] ;
              rdf:rest [ rdf:type swrl:AtomList ;
                         rdf:rest [ rdf:type swrl:AtomList ;
                                    rdf:first [ rdf:type swrl:ClassAtom ;
                                                swrl:classPredicate <http://purl.org/ceu/eco/1.0#Multiheteroset> ;
                                                swrl:argument1 <urn:swrl#collection>
                                              ] ;
                                    rdf:rest [ rdf:type swrl:AtomList ;
                                               rdf:rest rdf:nil ;
                                               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                           swrl:propertyPredicate <http://purl.org/ceu/simeon#usesComparisonMethod> ;
                                                           swrl:argument1 <urn:swrl#measure> ;
                                                           swrl:argument2 <urn:swrl#method>
                                                         ]
                                             ]
                                  ] ;
                         rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                     swrl:propertyPredicate <http://purl.org/ceu/simeon#hasSourceEntity> ;
                                     swrl:argument2 <urn:swrl#collection> ;
                                     swrl:argument1 <urn:swrl#measure>
                                   ]
                       ]
            ]
] .

