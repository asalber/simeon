#
#    Classes
#
#################################################################


###  http://purl.org/ceu/simeon/1.0#BooleansComparisonMethod

<http://purl.org/ceu/simeon/1.0#BooleansComparisonMethod> rdf:type owl:Class ;
                                                          
                                                          rdfs:label "Boolean comparison method"@en ,
                                                                     "Método de comparación de booleanos."@es ;
                                                          
                                                          rdfs:subClassOf <http://purl.org/ceu/simeon#DataComparisonMethod> .



###  http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod

<http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> rdf:type owl:Class ;
                                                             
                                                             rdfs:label "Collections comparison method"@en ,
                                                                        "Método de comparación de colecciones."@es ;
                                                             
                                                             rdfs:subClassOf <http://purl.org/ceu/simeon#ObjectComparisonMethod> ,
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty <http://purl.org/ceu/simeon#comparesObject> ;
                                                                               owl:allValuesFrom <http://purl.org/ceu/eco/1.0#Collection>
                                                                             ] ;
                                                             
                                                             rdfs:comment "A collections comparison method is a method to compare two collections, groups or aggregations of objects."@en ;
                                                             
                                                             rdfs:seeAlso "E-Collections ontology (http://aprendeconalf.es/ecollections)"@en ,
                                                                          "Ontología E-Collections (http://aprendeconalf.es/ecollections)"@es ;
                                                             
                                                             rdfs:comment "Un método de comparación de colecciones es un método para comparar dos colecciones, grupos o agregados de objetos."@es .



###  http://purl.org/ceu/simeon/1.0#CollectionsDissimilarityFunction

<http://purl.org/ceu/simeon/1.0#CollectionsDissimilarityFunction> rdf:type owl:Class ;
                                                                  
                                                                  rdfs:label "Collections dissimilarity function"@en ,
                                                                             "Función de disimilitud de colecciones."@es ;
                                                                  
                                                                  owl:equivalentClass [ rdf:type owl:Class ;
                                                                                        owl:intersectionOf ( <http://purl.org/ceu/simeon#DissimilarityFunction>
                                                                                                             <http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod>
                                                                                                           )
                                                                                      ] .



###  http://purl.org/ceu/simeon/1.0#CollectionsSimilarityFunction

<http://purl.org/ceu/simeon/1.0#CollectionsSimilarityFunction> rdf:type owl:Class ;
                                                               
                                                               rdfs:label "Collections similarity function"@en ,
                                                                          "Función de similitud de colecciones"@es ;
                                                               
                                                               owl:equivalentClass [ rdf:type owl:Class ;
                                                                                     owl:intersectionOf ( <http://purl.org/ceu/simeon#SimilarityFunction>
                                                                                                          <http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod>
                                                                                                        )
                                                                                   ] .



###  http://purl.org/ceu/simeon/collections#BooleansComparisonMeasure

:BooleansComparisonMeasure rdf:type owl:Class ;
                           
                           rdfs:subClassOf <http://purl.org/ceu/simeon#DataComparisonMeasure> ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://purl.org/ceu/simeon#hasSourceData> ;
                                             owl:someValuesFrom xsd:boolean
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://purl.org/ceu/simeon#hasTargetData> ;
                                             owl:someValuesFrom xsd:boolean
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://purl.org/ceu/simeon#usesComparisonMethod> ;
                                             owl:someValuesFrom <http://purl.org/ceu/simeon/1.0#BooleansComparisonMethod>
                                           ] .



