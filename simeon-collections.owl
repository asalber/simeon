AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#BooleanComparisonMethod> "Método de comparación de booleanos."@es)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#BooleanComparisonMethod> "Boolean comparison method"@en)
SubClassOf(<http://purl.org/ceu/simeon/1.0#BooleanComparisonMethod> <http://purl.org/ceu/simeon/1.0#SimpleDataComparisonMethod>)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#BooleanSimilarityFunction> "Boolean similarity function"@en)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#BooleanSimilarityFunction> "Función de similitud de booleanos"@es)
SubClassOf(<http://purl.org/ceu/simeon/1.0#BooleanSimilarityFunction> <http://purl.org/ceu/simeon/1.0#BooleanComparisonMethod>)
SubClassOf(<http://purl.org/ceu/simeon/1.0#BooleanSimilarityFunction> <http://purl.org/ceu/simeon/1.0#SimpleDataSimilarityFunction>)
AnnotationAssertion(rdfs:seeAlso <http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> "E-Collections ontology (http://aprendeconalf.es/ecollections)"@en)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> "Método de comparación de colecciones."@es)
AnnotationAssertion(rdfs:seeAlso <http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> "Ontología E-Collections (http://aprendeconalf.es/ecollections)"@es)
AnnotationAssertion(rdfs:comment <http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> "Un método de comparación de colecciones es un método para comparar dos colecciones, grupos o agregados de objetos."@es)
AnnotationAssertion(rdfs:comment <http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> "A collections comparison method is a method to compare two collections, groups or aggregations of objects."@en)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> "Collections comparison method"@en)
EquivalentClasses(<http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> ObjectIntersectionOf(ObjectHasValue(<http://purl.org/ceu/simeon#hasDomain> <http://purl.org/ceu/eco/1.0#Collection>) <http://purl.org/ceu/simeon#ComparisonMethod>))
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#CollectionsDissimilarityFunction> "Función de disimilitud de colecciones."@es)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#CollectionsDissimilarityFunction> "Collections dissimilarity function"@en)
EquivalentClasses(<http://purl.org/ceu/simeon/1.0#CollectionsDissimilarityFunction> ObjectIntersectionOf(<http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> <http://purl.org/ceu/simeon#DissimilarityFunction>))
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#CollectionsSimilarityFunction> "Función de similitud de colecciones"@es)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#CollectionsSimilarityFunction> "Collections similarity function"@en)
EquivalentClasses(<http://purl.org/ceu/simeon/1.0#CollectionsSimilarityFunction> ObjectIntersectionOf(<http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod> <http://purl.org/ceu/simeon#SimilarityFunction>))
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#MultiheterosetComparisonMethod> "Método de comparación de multiheteroconjuntos."@es)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#MultiheterosetComparisonMethod> "Multiheteroset comparison method"@en)
EquivalentClasses(<http://purl.org/ceu/simeon/1.0#MultiheterosetComparisonMethod> ObjectIntersectionOf(ObjectHasValue(<http://purl.org/ceu/simeon#hasDomain> <http://purl.org/ceu/eco/1.0#Multiheteroset>) <http://purl.org/ceu/simeon/1.0#CollectionsComparisonMethod>))
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#MultiheterosetSimilarityFunction> "Función de similitud de multiheteroconjuntos"@es)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#MultiheterosetSimilarityFunction> "Multiheterosets similarity function"@en)
EquivalentClasses(<http://purl.org/ceu/simeon/1.0#MultiheterosetSimilarityFunction> ObjectIntersectionOf(<http://purl.org/ceu/simeon/1.0#MultiheterosetComparisonMethod> <http://purl.org/ceu/simeon/1.0#CollectionsSimilarityFunction>))
EquivalentClasses(<http://purl.org/ceu/simeon/1.0#MultisetComparisonMethod> ObjectIntersectionOf(ObjectHasValue(<http://purl.org/ceu/simeon#hasDomain> <http://purl.org/ceu/eco/1.0#Multiset>) <http://purl.org/ceu/simeon/1.0#MultiheterosetComparisonMethod>))
EquivalentClasses(<http://purl.org/ceu/simeon/1.0#MultisetSimilarityFunction> ObjectIntersectionOf(<http://purl.org/ceu/simeon/1.0#MultisetComparisonMethod> <http://purl.org/ceu/simeon/1.0#MultiheterosetSimilarityFunction>))
AnnotationAssertion(rdfs:comment <http://purl.org/ceu/simeon/1.0#SimpleDataComparisonMethod> "A simple data comparison method is a method to compare two simple data of the same type (booleans, numbers, dates, strings, etc.)."@en)
AnnotationAssertion(rdfs:comment <http://purl.org/ceu/simeon/1.0#SimpleDataComparisonMethod> "Un método de comparación de datos simple is un método para comparar dos datos simples del mismo tipo (booleanos, números, fechas, cadenas, etc.)."@es)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#SimpleDataComparisonMethod> "Método de comparación de datos simples"@es)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#SimpleDataComparisonMethod> "Simple data comparison method"@en)
SubClassOf(<http://purl.org/ceu/simeon/1.0#SimpleDataComparisonMethod> <http://purl.org/ceu/simeon#ComparisonMethod>)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#SimpleDataDissimilarityFuncion> "Simple data dissimilarity function"@en)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#SimpleDataDissimilarityFuncion> "Función de disimilitud de datos simples"@es)
EquivalentClasses(<http://purl.org/ceu/simeon/1.0#SimpleDataDissimilarityFuncion> ObjectIntersectionOf(<http://purl.org/ceu/simeon/1.0#SimpleDataComparisonMethod> <http://purl.org/ceu/simeon#DissimilarityFunction>))
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#SimpleDataSimilarityFunction> "Función de similitud de datos simples"@es)
AnnotationAssertion(rdfs:label <http://purl.org/ceu/simeon/1.0#SimpleDataSimilarityFunction> "Simple data similiarity function"@en)
SubClassOf(<http://purl.org/ceu/simeon/1.0#SimpleDataSimilarityFunction> <http://purl.org/ceu/simeon/1.0#SimpleDataComparisonMethod>)
DLSafeRule(Body(ClassAtom(<http://purl.org/ceu/simeon#ComparisonMeasure> Variable(<urn:swrl#measure>)) ObjectPropertyAtom(<http://purl.org/ceu/simeon#hasSourceEntity> Variable(<urn:swrl#measure>) Variable(<urn:swrl#collection>)) ClassAtom(<http://purl.org/ceu/eco/1.0#Multiheteroset> Variable(<urn:swrl#collection>)) ObjectPropertyAtom(<http://purl.org/ceu/simeon#usesComparisonMethod> Variable(<urn:swrl#measure>) Variable(<urn:swrl#method>)))Head(ObjectPropertyAtom(<http://purl.org/ceu/simeon#hasDomain> Variable(<urn:swrl#method>) <http://purl.org/ceu/eco/1.0#Multiheteroset>)))
DLSafeRule(Body(ClassAtom(<http://purl.org/ceu/simeon#ComparisonMeasure> Variable(<urn:swrl#measure>)) ObjectPropertyAtom(<http://purl.org/ceu/simeon#usesComparisonMethod> Variable(<urn:swrl#measure>) Variable(<urn:swrl#method>)) ObjectPropertyAtom(<http://purl.org/ceu/simeon#hasDomain> Variable(<urn:swrl#method>) <http://purl.org/ceu/eco/1.0#Multiheteroset>) ObjectPropertyAtom(<http://purl.org/ceu/simeon#hasSourceEntity> Variable(<urn:swrl#measure>) Variable(<urn:swrl#collection>)))Head(ClassAtom(<http://purl.org/ceu/eco/1.0#Multiheteroset> Variable(<urn:swrl#collection>))))
